{
  "name": "react-native-notifier",
  "version": "1.6.109",
  "description": "Fast and simple in-app notifications for React Native",
  "main": "src/index",
  "module": "src/index",
  "react-native": "src/index",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "android",
    "ios",
    "!lib/typescript/example",
    "!android/build",
    "!ios/build",
    "!node_modules",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__"
  ],
  "scripts": {
    "test": "jest",
    "typescript": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "release": "release-it",
    "example": "yarn --cwd example",
    "pods": "cd example && pod-install --quiet",
    "bootstrap": "yarn example && yarn && yarn pods"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/lexgorbunov/react-native-notifier",
  "author": "Alexander Gorbunov <lexgorbunov@gmail.com> (https://github.com/lexgorbunov)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lexgorbunov/react-native-notifier/issues"
  },
  "homepage": "https://github.com/lexgorbunov/react-native-notifier#readme",
  "devDependencies": {
    "@commitlint/config-conventional": "^11.0.0",
    "@react-native-community/bob": "^0.10.0",
    "@react-native-community/eslint-config": "^0.0.7",
    "@release-it/conventional-changelog": "^2.0.0",
    "@types/jest": "^25.1.2",
    "@types/react": "^16.9.19",
    "@types/react-native": "0.61.10",
    "commitlint": "^11.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "husky": "^4.0.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "react": "~16.9.0",
    "react-native": "~0.61.5",
    "react-native-gesture-handler": "^1.6.1",
    "release-it": "^14.3.0",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-gesture-handler": "*"
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  }
}
